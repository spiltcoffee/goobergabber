    </em:targetApplication>

    <em:iconURL>chrome://goobergabber/skin/icon48.png</em:iconURL>
    <em:icon64URL>chrome://goobergabber/skin/icon64.png</em:icon64URL>

    <!-- Front End MetaData -->
    <em:name>#GooberGabber - GamerGate Word Scrambler</em:name>
